<h3>Clients</h3>
<hr>
<table class="table tables-striped">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Last Viewed
            </th>
        </tr>
    </thead>
    <tbody>
        {{#each model as |client|}}
            <tr>
                <td>
                		{{link-to client.name 'clients.show' client}}
                </td>
                <td>
                    {{client.lastViewed}}
                </td>
            </tr>
        {{/each}}
    </tbody>
</table>
{{outlet}}
